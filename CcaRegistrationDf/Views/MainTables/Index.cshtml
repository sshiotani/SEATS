@model IEnumerable<CcaRegistrationDf.Models.MainTable>

@{
    ViewBag.Title = "Index";
}

<h2>Applications</h2>

<p>
    @Html.ActionLink("Register For New Online Course", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationSubmissionDate)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.StudentLastName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.StudentFirstName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.CompletionStatus)
        </th>
       
      

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ApplicationSubmissionDate)
        </td>
      
        <th>
            @Html.DisplayFor(modelItem => item.StudentLastName)
        </th>

        <th>
            @Html.DisplayFor(modelItem => item.StudentFirstName)
        </th>

        <td>
            @Html.DisplayFor(modelItem => item.CompletionStatus)
        </td>
       
    </tr>
}

</table>
